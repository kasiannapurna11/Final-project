                <button class="btn btn-outline-success" onclick="logoutUser()">LogOut</button>

            </div>
        </div>
    </nav>
    <div id="header"></div>
    <main class='d-flex flex-column align-items-center' id="image-books">

        <table class="table mt-4 table-fluid" id="myTable">
            <thead>
                <tr>
                    <th scope="col">BookID</th>
                    <th scope="col">UserEmail</th>
                    <th scope="col">Period(Days)</th>
                    <th scope="col">IssueDate</th>
                </tr>
            </thead>
            <tbody>
                <%issueList.forEach(issue=> {%>
                    <tr>
                        <th scope="row">
                            <%= issue.bookid %>
                        </th>
                        <td>
                            <%= issue.useremail %>
                        </td>
                        <td>
                            <%= issue.period %>
                        </td>
                        <td>
                            <%= issue.date%>
                        </td>
                    </tr>
                    <%})%>
            </tbody>
        </table>
    </main>
    <script>
        $(document).ready(function () {
            $('#myTable').DataTable();
        });
    </script>
</body>
